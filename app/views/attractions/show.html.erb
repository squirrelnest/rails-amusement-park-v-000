<% if !@user.admin %>
  <h1><%= @attraction.name %></h1>
  <%= button_to "Go on this ride", "/rides/create/#{@attraction.id}/#{@user.id}" %>
<% end %>

<% if @user.admin %>
  <%= link_to "Edit Attraction", edit_attraction_path(@attraction) %>
  <% @attraction.attributes.each do |key, value| %>
    <%= key %>: <%= value %>
    <br>
  <% end %>
<% end %>
